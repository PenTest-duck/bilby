# Environment configuration for Bilby backend

# Node environment
NODE_ENV=development

# Redis connection
REDIS_URL=redis://localhost:6379

# TfNSW API
TFNSW_API_KEY=your_tfnsw_api_key_here

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key_here
SUPABASE_SECRET_KEY=your_supabase_service_role_key_here

# Expo
EXPO_ACCESS_TOKEN=your_expo_access_token_here
EXPO_PUBLIC_PROJECT_ID=your_expo_project_id_here

# Feature Flags
# Enable GraphQL fare enrichment (unofficial API, may be unstable)
ENABLE_GRAPHQL_FARES=true
